{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Grpc": "Error",
        "ProtoBuf": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-mm-dd HH:mm:ss.fff} {Level:u3}] [{MachineName}] [{SourceContext}] {Message}{NewLine:1}{Exception:1}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "KSociety.Example"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
  "DebugFlag": true,
  "MessageBroker": {
    "ConnectionFactory": {
      "MqHostName": "localhost",
      "MqUserName": "KSociety",
      "MqPassword": "KSociety"
    },
    "ExchangeDeclareParameters": {
      "BrokerName": "k-society_example",
      "ExchangeType": 1,
      "ExchangeDurable": false,
      "ExchangeAutoDelete": true
    },
    "QueueDeclareParameters": {
      "QueueDurable": false,
      "QueueExclusive": false,
      "QueueAutoDelete": true
    }
  }
}
